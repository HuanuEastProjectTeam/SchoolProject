<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".FromCar">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/huibaise"
        >
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="100dp"
            android:orientation="horizontal"
            android:background="@color/heise"
            >
            <Button
                android:id="@+id/fanhui"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="&lt;  返回"
                android:textSize="16sp"
                android:textColor="@color/white"
                android:background="@color/transparent"
                android:paddingTop="20dp"
                style="?android:attr/borderlessButtonStyle"
                android:onClick="PublishFrom"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:paddingTop="20dp"
                android:textSize="18sp"

                android:textStyle="bold"
                android:textColor="@color/white"
                android:text="填写停车位信息"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/carOne"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="@drawable/bnav_common_cp_refresh_button_selector"
                android:text="请完善以下信息"
                android:gravity="center"
                android:layout_marginTop="70dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                />
        </RelativeLayout>
    </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_below="@id/carOne"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/bnav_common_cp_refresh_button_selector"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/huibaise"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="50dp"
                    android:id="@+id/carThree"
                    android:text="车位号"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="20dp"
                    />
                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:ems="10"
                    android:background="@null"
                    android:id="@+id/carNumber"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/huibaise"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="50dp"
                    android:id="@+id/carFore"

                    android:text="联系电话"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="20dp"
                    />
                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:ems="10"
                    android:background="@null"
                    android:id="@+id/phoneNumber"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/huibaise"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="90dp"
                    android:layout_height="50dp"
                    android:id="@+id/carFive"

                    android:text="空闲开始时间"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="20dp"
                    />
                <TimePicker
                    android:id="@+id/StartTimePicker"
                    android:timePickerMode="spinner"
                    android:layout_width="wrap_content"
                    android:layout_height="90dp"/>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/huibaise"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="90dp"
                    android:layout_height="50dp"
                    android:id="@+id/carSix"

                    android:text="空闲结束时间"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="20dp"
                    />
                <TimePicker
                    android:id="@+id/EndTimePicker"
                    android:timePickerMode="spinner"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/navi_dimens_90dp"/>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/huibaise"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="50dp"
                    android:id="@+id/carSeven"
                    android:text="收费标准"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="20dp"
                    />
                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:ems="9"
                    android:background="@null"
                    android:id="@+id/moneyCar"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="元/时"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/huibaise"
                />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="105dp"
                android:gravity="center"
                >
                <Button
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:text="发布订单"
                    android:background="@color/cc"
                    android:gravity="center"
                    android:onClick="PublishFrom"
                    android:id="@+id/publish"
                    />
            </RelativeLayout>


        </LinearLayout>

</LinearLayout>